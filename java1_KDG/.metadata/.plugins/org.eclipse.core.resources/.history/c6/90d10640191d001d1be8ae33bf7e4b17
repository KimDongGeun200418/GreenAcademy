package sub1;
/*
 * 
 * 날짜 : 2022/08/16
 * 이름 : 김동근
 * 내용 : Java 변수 실습하기
 * 
 * */
public class OperatorTest {
	
	public static void main(String[] args) {
		
		//변수
		
		String hello = "Hello World!";
		int a = 3;
		int b = 7;
		
		System.out.println("hello : " + hello);
		System.out.println("a : " + a);
		System.out.println("b : " + b);
		
		hello = "Hello Busan!";
		a = 7;
		b = 3;
		
		System.out.println("hello : " + hello);
		System.out.println("a : " + a);
		System.out.println("b : " + b);
		
		//상수
		/*대문자 표기
		 */
		
		final int NUM = 5;
		//NUM = 7;
		System.out.println(NUM);
		
		
		
		
	}
	
}