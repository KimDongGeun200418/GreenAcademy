public class HelloWorld {

	public static void main(String[] args) {
		
		
		// 기본 출력
		System.out.println("Hello World!");
		
		// 한줄 출력
		System.out.print("Hello");
		System.out.print("World!");
		
		// 서식 출력
		System.out.println());
		System.out.printf("%s %s", "Hello", "Busan!");
		
	}
	
}
